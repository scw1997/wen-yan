{
  "name": "wen-yan",
  "version": "1.0.0",
  "description": "Vue template for front-end framework Swico",
  "main": "index.js",
  "scripts": {
    "start": "swico start",
    "build": "swico build",
    "build:analyze": "cross-env ANALYZE=true swico build",
    "lint:fix": "eslint --fix src/**/*.{js,ts,vue} && prettier --write src/**/*.{js,ts,vue,css,less,scss,json,html,md}",
    "husky:init": "npx husky init",
    "test": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{css,less,scss,json,html,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scw1997/swico.git"
  },
  "keywords": [
    "swico-template-vue",
    "swico",
    "typescript",
    "vue",
    "javascript",
    "rspack"
  ],
  "author": "scw",
  "license": "ISC",
  "dependencies": {
    "vue": "~3.5.13",
    "swico": "2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "10.3.7",
    "@types/node": "22.14.1",
    "globals": "16.0.0",
    "eslint": "9.24.0",
    "eslint-plugin-vue": "10.0.0",
    "vue-eslint-parser": "10.1.3",
    "typescript-eslint": "8.30.1",
    "@eslint/js": "9.24.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "cross-env": "7.0.3",
    "husky": "9.0.10",
    "@commitlint/cli": "18.5.0",
    "@commitlint/config-conventional": "18.5.0",
    "lint-staged": "15.2.1",
    "@rspack/core": "1.5.6"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
